			"lg": 12,
			"label":"Unit",
			"defaultFunc": "(customObj && customObj.common && customObj.common.unit) || ''"
		},
		"unit_zero": {
			"type": "text",
			"sm": 12,
			"lg": 12,
			"label":"Unit (if value is 0)",
			"help": "(this is only supported for numbers)",
			"defaultFunc": "(customObj && customObj.common && customObj.common.unit) || ''"
		},
		"unit_one": {
			"type": "text",
